{
  "name": "txt2epub",
  "version": "1.0.0",
  "description": "convert text files to epub",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "gitlog": "git log `git describe --tags --abbrev=0`..HEAD --no-merges --graph --decorate --pretty=format:'%h %Cblue%s%Creset %cr (%an)'",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier-eslint --list-different src/**/*.js",
    "format-danger": "prettier-eslint --write src/**/*.js",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/someok/txt2epub.git"
  },
  "keywords": [
    "txt",
    "epub"
  ],
  "author": "JX Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/someok/txt2epub/issues"
  },
  "homepage": "https://github.com/someok/txt2epub#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "inquirer": "^6.3.1",
    "fs-extra": "^7.0.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "stat-mode": "^0.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@commitlint/cli": "^7.6.0",
    "@commitlint/config-conventional": "^7.6.0",
    "babel-eslint": "^10.0.1",
    "jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "prettier-eslint-cli": "^4.7.1",
    "standard-version": "^6.0.1"
  }
}
