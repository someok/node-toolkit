{
  "name": "@someok/txt2epub",
  "version": "1.1.1",
  "description": "convert text files to epub",
  "bin": {
    "txt2epub": "./dist/cli/index.js"
  },
  "main": "./dist/cli/index.js",
  "scripts": {
    "test": "jest",
    "compile": "tsc",
    "gitlog": "git log `git describe --tags --abbrev=0`..HEAD --no-merges --graph --decorate --pretty=format:'%h %Cblue%s%Creset %cr (%an)'",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier-eslint --list-different src/**/*.js",
    "format-danger": "prettier-eslint --write src/**/*.js",
    "release": "standard-version",
    "publish2npm": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/someok/txt2epub.git"
  },
  "keywords": [
    "txt",
    "epub"
  ],
  "author": "JX Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/someok/txt2epub/issues"
  },
  "homepage": "https://github.com/someok/txt2epub#readme",
  "dependencies": {
    "archiver": "^3.0.0",
    "chalk": "^2.4.2",
    "ejs": "^2.6.1",
    "fs-extra": "^7.0.1",
    "iconv": "^2.3.4",
    "inquirer": "^6.3.1",
    "js-yaml": "^3.13.1",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.11",
    "marked": "^0.6.2",
    "pinyinlite": "^1.2.1",
    "stat-mode": "^0.3.0",
    "uuid": "^3.3.2",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.6.0",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/archiver": "^2.1.3",
    "@types/ejs": "^2.6.3",
    "@types/fs-extra": "^7.0.0",
    "@types/inquirer": "^6.0.2",
    "@types/jest": "^24.0.12",
    "@types/js-yaml": "^3.12.1",
    "@types/klaw-sync": "^6.0.0",
    "@types/lodash": "^4.14.129",
    "@types/marked": "^0.6.5",
    "@types/node": "^12.0.0",
    "@types/stat-mode": "^0.2.0",
    "@types/uuid": "^3.4.4",
    "@types/yauzl": "^2.9.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "commander": "^2.20.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "prettier-eslint-cli": "^4.7.1",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.5"
  },
  "files": [
    "dist/",
    "epub-boilerplate/"
  ]
}
