{
  "name": "node-toolkit",
  "private": true,
  "version": "1.3.1",
  "description": "someok node toolkit",
  "author": "JX Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/someok/node-toolkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/someok/node-toolkit.git"
  },
  "homepage": "https://github.com/someok/node-toolkit#readme",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "clean": "yarn workspaces run clean",
    "commit": "commit",
    "version": "lerna version",
    "build": "lerna run --stream build",
    "build:node-utils": "yarn workspace @someok/node-utils build",
    "build:txt-to": "yarn workspace @someok/txt-to build",
    "build:comic-spider": "yarn workspace @someok/comic-spider build",
    "test": "yarn run build && jest",
    "test:node-utils": "yarn workspace @someok/node-utils test",
    "test:txt-to": "yarn workspace @someok/txt-to test",
    "test:comic-spider": "yarn workspace @someok/comic-spider test",
    "publish": "lerna publish from-git",
    "publish:node-utils": "yarn workspace @someok/node-utils run publish2npm",
    "publish:txt-to": "yarn workspace @someok/txt-to run publish2npm",
    "publish:comic-spider": "yarn workspace @someok/comic-spider run publish2npm",
    "gitlog": "git log `git describe --tags --abbrev=0`..HEAD --no-merges --graph --decorate --pretty=format:'%h %Cblue%s%Creset %cr (%an)'",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint 'packages/**/src/**/*.{js,ts,tsx}'",
    "format": "eslint --fix 'packages/**/src/**/*.{js,ts,tsx}'",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@commitlint/prompt-cli": "^12.1.1",
    "@types/cheerio": "^0.22.28",
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.23",
    "@types/klaw-sync": "^6.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.2",
    "@types/prompts": "^2.0.11",
    "@types/stat-mode": "^0.2.0",
    "@types/tunnel": "^0.0.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.6.3",
    "codecov": "^3.8.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "26.6.3",
    "lerna": "4.0.0",
    "lint-staged": "^11.0.0",
    "nock": "^13.0.11",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
