{
  "name": "txt-to",
  "private": true,
  "version": "1.3.0",
  "description": "convert txt to epub, pdf, html etc",
  "author": "JX Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/someok/txt-to/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/someok/txt-to.git"
  },
  "homepage": "https://github.com/someok/txt-to#readme",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:node-utils": "yarn workspace @someok/node-utils test",
    "test:txt-to": "yarn workspace @someok/txt-to test",
    "compile:node-utils": "yarn workspace @someok/node-utils compile",
    "compile:txt-to": "yarn workspace @someok/txt-to compile",
    "publish:node-utils": "yarn workspace @someok/node-utils publish2npm",
    "publish:txt-to": "yarn workspace @someok/txt-to publish2npm",
    "release": "standard-version",
    "release-patch": "standard-version --release-as patch",
    "gitlog": "git log `git describe --tags --abbrev=0`..HEAD --no-merges --graph --decorate --pretty=format:'%h %Cblue%s%Creset %cr (%an)'",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "lint": "eslint 'packages/**/src/**/*.{js,ts,tsx}'",
    "format": "eslint --fix 'packages/**/src/**/*.{js,ts,tsx}'"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "chardet": "^0.7.0",
    "ejs": "^2.6.1",
    "encoding": "^0.1.12",
    "fs-extra": "^8.0.1",
    "inquirer": "^6.3.1",
    "js-yaml": "^3.13.1",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.11",
    "marked": "^0.6.2",
    "pureimage": "^0.1.6",
    "stat-mode": "^0.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/archiver": "^3.0.0",
    "@types/chardet": "^0.5.0",
    "@types/ejs": "^2.6.3",
    "@types/fs-extra": "^7.0.0",
    "@types/inquirer": "^6.0.3",
    "@types/jest": "^24.0.13",
    "@types/js-yaml": "^3.12.1",
    "@types/klaw-sync": "^6.0.0",
    "@types/lodash": "^4.14.132",
    "@types/marked": "^0.6.5",
    "@types/node": "^12.0.2",
    "@types/stat-mode": "^0.2.0",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "commander": "^2.20.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  }
}
